ipmi:
  username: ADMIN    # Example only - change to your credentials
  password: ADMIN    # Example only - change to your credentials

servers: []

discovery:
  netman_url: "http://network.g10.lo"
  ip_range_min: 10
  ip_range_max: 199

reboot_detection:
  sol_patterns:
    - "POST"
    - "BIOS"
    - "Booting"
  chassis_poll_interval: 30s

logs:
  path: /var/lib/data/logs
  retention_days: 30

server:
  port: 80
